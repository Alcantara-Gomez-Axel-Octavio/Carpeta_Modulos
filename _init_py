class CalcBasica:

    def resta(*nums):
        res = 0
        for valor in nums:
            res-= valor
        return res 
    
    def suma(*nums):
        res = 0
        for valor in nums:
            res += valor
        return sum 
    
    def multiplicacion(*nums):
        res = 0
        for valor in nums:
            res*= valor
        return mult 
    
    def divi(*nums):
        div = 0
        for valor in nums:
            div/= valor
        return div   